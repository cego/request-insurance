{
    "name": "cego/request-insurance",
    "description": "Package for Laravel that facilitates and ensures that http requests are sent",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@creationsftw.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-curl": "*",
        "nesbot/carbon": "^2.17",
        "nbj/stopwatch": "^0.0.5",
        "illuminate/view": "11.0.6",
        "illuminate/http": "11.0.6",
        "illuminate/routing": "11.0.6",
        "illuminate/support": "11.0.6",
        "illuminate/console": "11.0.6",
        "illuminate/database": "11.0.6",
        "illuminate/pagination": "11.0.6",
        "illuminate/encryption": "11.0.6",
        "guzzlehttp/guzzle": "^6.5.5|^7.2",
        "jfcherng/php-diff": "^6.13",
        "doctrine/dbal": "^3.3"
    },
    "require-dev": {
        "cego/php-cs-fixer": "^2.0",
        "orchestra/testbench": "^6.18",
        "open-telemetry/sdk": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cego\\": "src/"
        },
        "files": [
            "src/RequestInsurance/OpenTelemetry/_register.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cego\\RequestInsurance\\RequestInsuranceServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
